# O(n*(slogs)) / O(n*s)
class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        
        a_groups = defaultdict(list)
        
        for s in strs:
            a_key = ''.join(sorted(s))
            a_groups[a_key].append(s)
        
        return a_groups.values()
